<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Beans.Direccion" table="DIRECCION" catalog="2aDISTRIBUCION" optimistic-lock="version">
        <id name="codDirec" type="java.lang.Short">
            <column name="Cod_Direc" />
            <generator class="identity" />
        </id>
        <many-to-one name="ciudadConcp" class="Beans.CiudadConcp" fetch="select">
            <column name="Cod_Ciudad" not-null="false" />
            <column name="Cod_Postal" length="5" not-null="false" />
        </many-to-one>
        <property name="nombre" type="string">
            <column name="Nombre" length="75" not-null="false" />
        </property>
        <property name="numero" type="short">
            <column name="Numero" not-null="false" />
        </property>
        <set name="empleados" table="EMPLEADO" inverse="true" lazy="false" fetch="select">
            <key>
                <column name="Cod_Direc" />
            </key>
            <one-to-many class="Beans.Empleado" />
        </set>
        <set name="locals" table="LOCAL" inverse="true" lazy="false" fetch="select">
            <key>
                <column name="Cod_Direc" not-null="true" />
            </key>
            <one-to-many class="Beans.Local" />
        </set>
    </class>
</hibernate-mapping>
